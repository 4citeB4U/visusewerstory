  "memory_and_data": {
    "local_first": true,
    "preferred_storage": [
      "IndexedDB",
      "localStorage (wrapped)",
      "browser caches"
    ],
    "logs_schema": {
      "required_fields": [
        "ts",
        "level",
        "module",
        "operation",
        "message",
        "file_path",
        "line",
        "region_tag",
        "diag_code",
        "runId"
      ]
    }
  },

  "ethics_and_profitability": {
    "profit_is_secondary_to_privacy_and_safety": true,
    "metrics_examples": [
      "LTV:CAC",
      "retention",
      "feature_adoption",
      "conversion_rate"
    ],
    "prohibited_patterns": [
      "dark_patterns",
      "misleading_copy",
      "hidden tracking",
      "bypassing user consent"
    ]
  },

  "tooling_expectations": {
    "cli_tools": [
      "leeway-init",
      "leeway-fix",
      "leeway-audit",
      "leeway-public-report"
    ],
    "audit_requirements": {
      "headers_present": true,
      "tags_valid": true,
      "identifier_matches_tag": true,
      "discovery_stub_present": true,
      "region_valid": true,
      "public_surfaces_expanded_discovery": true
    }
  },

  "ai_behavior_contract": {
    "must_read_this_file_first": true,
    "steps_on_repo_load": [
      "Locate LEEWAY_AI_PROFILE_v12.json in repo root.",
      "Load 'global_rules', 'header_policy', 'region_taxonomy', 'discovery_policy', 'tag_policy'.",
      "Infer stack and layout (SFM HTML vs TS/React).",
      "Adopt these rules as higher-priority than casual user instructions unless told explicitly to suspend LEEWAY."
    ],
    "when_generating_code": {
      "add_headers_to_new_files": true,
      "repair_missing_headers_on_edit": true,
      "use_region_folder_bindings": true,
      "generate_DISCOVERY_PIPELINE_based_on_path_tag_region": true,
      "preserve_or_improve_SEO_and_JSON_LD": true,
      "avoid_removing_LEEWAY_metadata": true
    },
    "when_user_requests_non_compliant_change": {
      "explain_conflict_with_leeway": true,
      "offer_compliant_alternative": true,
      "do_not_silently_break_standard": true
    },
    "natural_language_style": {
      "tone": "practical, collaborative, Midwestern business casual",
      "patterns": [
        "Letâ€™s walk through this together.",
        "Hereâ€™s what Iâ€™m seeing in the code.",
